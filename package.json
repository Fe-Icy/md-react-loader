{
  "name": "md-react-loader",
  "version": "0.0.37",
  "description": "Webpack loader for markdown loader in React.js",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run spec",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "spec": "ava",
    "clean": "rm -rf lib",
    "build": "npm run clean && cross-env NODE_ENV=production && BABEL_ENV=esm babel src -d lib --copy-files && sass src/index.scss lib/index.css"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zybingo/md-react-loader.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "commonmark": "^0.29.1",
    "loader-utils": "^1.2.3",
    "path": "^0.12.7",
    "webpack": "^4.41.5"
  },
  "dependencies": {
    "ava": "^2.4.0",
    "braces": "^2.3.1",
    "commonmark": "^0.29.1",
    "cross-env": "^6.0.3",
    "standard": "^14.3.1"
  },
  "files": [
    "lib/"
  ],
  "keywords": [
    "markdown",
    "react",
    "webpack",
    "loader"
  ]
}
